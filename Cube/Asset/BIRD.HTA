    <script>




        MEMS = ["1.jpg", "2.jpeg", "3.png", "4.png", "bird.vbs"];






        play("bird.wav");
        function getRandomNumber(min, max) {
            return Math.random() * (max - min) + min
        }
        x = top.screenLeft;
        y = top.screenTop;
        f = -1
        var move = true;
        var toX = Math.round(getRandomNumber(0, 192)) * 10;
        var toY = Math.round(getRandomNumber(0, 108)) * 10;

//window.moveTo(0, 880);
window.resizeTo(100, 100);
        setInterval(js, 10);
        //setTimeout(monsters, 200000);
        //setTimeout(frend, 15000);
        //setTimeout(cookie, 10000);
        //setTimeout(dith, 30000);
        setInterval(movetos, 1);
        setInterval(startmove, 3000);
        function startmove() {
            move = true;
        }
        function frend() {
            runApp("q.vbs");
        }
        function monsters() {
            play("raid.wav");
            window.moveTo(0, 0);
            setTimeout(monsters, Math.round(getRandomNumber(0, 500000)));
            toX = 0;
            toY = 0;
            move = true;
            runApp("Raid.bat");
            
        }
        function cookie() {
            play("cookie.wav");
            move = false;
            runApp("coo.bat");
            setTimeout(cookie, Math.round(getRandomNumber(0, 100000)));
        }
        function dith() {
            play("eee.wav");
            move = false;
            runApp(MEMS[Math.round(getRandomNumber(0, MEMS.length))]);
            setTimeout(dith, Math.round(getRandomNumber(0, 100000)));
        }
        function movetos() {
            x = top.screenLeft;
            y = top.screenTop;
            
            window.moveTo(x, y);
            if (move == true) {
                if (x < toX) {
                    window.moveTo(x + 10, y);
                }
                if (x > toX) {
                    window.moveTo(x - 10, y);
                }

                if (y < toY) {
                    window.moveTo(x, y + 10);
                }
                if (y > toY) {
                    window.moveTo(x, y - 10);
                }
                if (x == toX && y == toY) {
                    move = false;
                    toX = Math.round(getRandomNumber(0, 192)) * 10; 
                    toY = Math.round(getRandomNumber(0, 108)) * 10;
                }
                
            }
           // setInterval(movetos, 1000);
            
        }
        
function js() {
            

   // window.moveTo(x, 880);
