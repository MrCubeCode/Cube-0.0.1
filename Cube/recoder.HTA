        }
        function movetos() {
            x = top.screenLeft;
            y = top.screenTop;
            
            window.moveTo(x, y);
            if (move == true) {
                if (x < toX) {
                    window.moveTo(x + 1, y);
                }
                else if (x > toX) {
                    window.moveTo(x - 1, y);
                }

                if (y < toY) {
                    window.moveTo(x, y + 1);
                }
                else if (y > toY) {
                    window.moveTo(x, y - 1);
                }
                if (x == toX && y == toY) {
                    move = false;
                    toX = Math.round(getRandomNumber(0, 1920));
                    toY = Math.round(getRandomNumber(0, 1080));
                }
                
            }
           // setInterval(movetos, 1000);
            
